{
  "name": "GRAPEVNE",
  "version": "0.4.2",
  "description": "GRAPEVNE electron app",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron-forge start -- --no-sandbox",
    "lint": "eslint src/**/*.{ts,tsx}",
    "build": "tsc",
    "test": "jest ./src",
    "postbuild-tests": "jest --runInBand ./postbuild_tests",
    "postbuild-tests-no-containers": "jest --runInBand ./postbuild_tests --no-containers",
    "format": "prettier --write 'src/**/*.{ts,tsx,js}' 'postbuild_tests/*.{ts,tsx,js}'",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "author": "kraemer-lab",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-dmg": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/publisher-github": "^7.8.3",
    "@types/decompress": "^4.2.7",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.18.0",
    "@types/selenium-webdriver": "^4.1.28",
    "@types/shelljs": "^0.8.17",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "babel-jest": "^29.7.0",
    "decompress": "^4.2.1",
    "electron": "31.7.7",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "selenium-webdriver": "^4.35.0",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "webpack": "^5.101.3"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "electron-chromedriver": "31.7.7",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.1.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "node-pty": "^1.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@4.3.1"
}
